10 CLS
20 OPEN "COM1:": PRINT #1, "Memory Map"
30 CS=&H0100
40 CE=PEEK(&H7FFF)*256+PEEK(&H7FFE)-1
50 LOCATE 1,0: PRINT "Code:"
60 A$=RIGHT$("000"+HEX$(CS),4)+ " - "
70 B$=RIGHT$("000"+HEX$(CE),4)
80 DIM S$(0)*12
90 LOCATE 7,0: PRINT A$
100 LOCATE 14,0: PRINT B$
110 PRINT #1, "Code:"+A$+ " - "+B$
120 DS=CE+1
130 DE=PEEK(&H7974)*256+PEEK(&H7973)-1
140 LOCATE 1,1: PRINT "RAMD:"
150 A$=RIGHT$("000"+HEX$(DS),4)+ " - "
160 B$=RIGHT$("000"+HEX$(DE),4)
170 LOCATE 7,1: PRINT A$
180 LOCATE 14,1: PRINT B$
190 PRINT #1, "RAMD:"+A$+ " - "+B$
200 FE=PEEK(DS+19)*256+PEEK(DS+18)+DS
230 WHILE FE<DE
240 FT=PEEK(FE)
290 N=PEEK(FE+18)*256+PEEK(FE+17)
320 N=N+FE
350 S$(0)=""
360 FOR I=1 TO 8
370 X=PEEK(FE+I)
380 IF X>32 THEN
390 S$(0)=S$(0)+CHR$(X)
400 ENDIF
410 NEXT
420 S$(0)=S$(0)+"."
430 FOR I=9 TO 11
440 X=PEEK(FE+I)
450 IF X>32 THEN
460 S$(0)=S$(0)+CHR$(X)
470 ENDIF
480 NEXT
490 PRINT HEX$(FE)+": "+S$(0)+" ("+HEX$(FT)+") Len: "+STR$(N-FE-34)
500 PRINT #1,HEX$(FE)+": "+S$(0)+" ("+HEX$(FT)+") Len: "+STR$(N-FE-34)
510 FE=N
520 WEND

