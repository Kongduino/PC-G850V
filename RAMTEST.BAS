10 RB=&H0106: ADDR=&H0123
20 B2R=&H038E: R2B=&H03A3
30 ST=&H8000
40 DIM X$(0)*24

50 A=&H61
60 FOR I=0 TO 23
70 POKE (RB+I),A
80 A=A+1
90 NEXT
100 A=INT(ST/256)
110 POKE (ADDR),ST-A*256
120 POKE (ADDR+1),A
130 CALL R2B

140*DSP24: A=INT(ST/256)
150 POKE (ADDR),ST-A*256
160 POKE (ADDR+1),A
170 CALL B2R

180 CLS
190 FOR J=0 TO 5
200 H$=HEX$(ST+J*4)+": "
210 T$="| "
220 X$(0)=""
230 FOR I=0 TO 3
240 A=PEEK(RB+J*4+I)
250 IF A<16 THEN
260 X$(0)=X$(0)+"0"
270 ENDIF
280 IF A<16 OR A>126 THEN
290 T$=T$+"."
300 ELSE
310 T$=T$+CHR$(A)
320 ENDIF

330 X$(0)=X$(0)+HEX$(A)+" "
350 NEXT
360 LOCATE 0,J: PRINT H$+X$(0)+T$
370 NEXT
390*WEHT: H$=INKEY$
400 IF H$="" THEN *WEHT
410 IF H$="J" THEN
420 ST=ST+24
430 GOTO *DSP24
440 ENDIF
450 IF H$="Q" THEN *THEEND
460 IF H$="K" THEN
470 ST=ST-24
480 IF ST<&H8000 THEN ST=&H8000
490 GOTO *DSP24
500 ENDIF

510 GOTO *DSP24
520*THEEND: END

