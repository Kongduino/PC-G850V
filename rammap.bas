20 OPEN "COM1:": PRINT #1, CHR$(10)+CHR$(10)+"LS"
30 CS=&H07000
40 DIM S$(0)*64
50 DIM T$(0)*16
60 FOR I=0 TO 255
70 S$(0)=HEX$(CS)+": "
80 T$(0)=" | "
90 FOR J=0 TO 15
100 A=PEEK(CS+J)
110 IF A<16 THEN
120 S$(0)=S$(0)+"0"+HEX$(A)+" "
130 T$(0)=T$(0)+"."
140 ELSE
150 S$(0)=S$(0)+HEX$(A)+" "
160 T$(0)=T$(0)+CHR$(A)
170 ENDIF
180 NEXT
190 LOCATE 0,0: PRINT S$(0)+T$(0)
200 PRINT #1,S$(0)+T$(0)
65 CS=CS+16
210 NEXT
220 CLOSE

